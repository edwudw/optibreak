{
  "manifest_version": 3,
  "name": "OptiBreak",
  "version": "0.3.0",
  "description": "Sends a notification every 20 minutes to look away at an object 20 feet away for 20 seconds.",
  "background": {
    "service_worker": "background.js"
  },
  "key": "",
  "permissions": [
    "notifications",
    "alarms",
    "storage",
    "identity"
  ],
  "oauth2": {
    "client_id": "781933626267-lbpfj3ugio8l9gi0dqlov9r0keevn94r.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/calendar.readonly"
    ]
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "128": "icon128.png"
    }
  },
  "options_page": "options.html",
  "icons": {
    "128": "icon128.png"
  }
}
